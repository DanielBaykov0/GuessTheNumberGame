<thlogic>
    <attr sel="#headId"
          th:replace="~{fragments/head :: headFragment}"/>
    <attr sel="#result-message"
          th:text="${resultMessage}"/>
    <attr sel="#restartId"
          th:href="restart"/>
    <attr sel="#localhostId"
          th:href="@{http://localhost:8080/}"/>
    <attr sel="#footerId"
          th:replace="~{fragments/footer :: footer}"/>
</thlogic>
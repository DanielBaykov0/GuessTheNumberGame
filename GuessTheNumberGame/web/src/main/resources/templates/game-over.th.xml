<thlogic>
    <attr sel="#headId"
          th:replace="~{fragments/head :: headFragment}"/>
    <attr sel="#game-title"
          th:text="#{game.title}"/>
    <attr sel="#game-over"
          th:text="#{game.over}"/>
    <attr sel="#result-message"
          th:text="${resultMessage}"/>
    <attr sel="#restartId"
          th:href="restart"
          th:text="#{game.play}"/>
    <attr sel="#localhostId"
          th:href="@{http://localhost:8080/}"
          th:text="#{game.home}"/>
    <attr sel="#footerId"
          th:replace="~{fragments/footer :: footer}"/>
</thlogic>